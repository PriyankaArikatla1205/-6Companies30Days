class Solution {
    public int numJewelsInStones(String jewels, String stones) {
                char[] ch = jewels.toCharArray();
        char[] ch1 = stones.toCharArray();
        int k=0;
        for(int i=0;i<ch1.length;i++){
            for(int j=0;j<ch.length;j++){
                if(ch1[i]==ch[j]){
                    k++;
                }
            }
        }
return k;
    }
}
